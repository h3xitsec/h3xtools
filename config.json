{
    "dockerTag":"h3xtools",
    "installDir":"/opt/h3xtools/bin",
    "linkDir":"/usr/local/bin",
    "tools": [
        {
            "name":"nmap",
            "image":"webrecon",
            "command":["nmap"],
            "usermode":false
        },
        {
            "name":"nikto",
            "image":"webrecon",
            "command":["nikto"],
            "usermode":false
        },
        {
            "name":"nmapAutomator",
            "image":"webrecon",
            "command":["nmapAutomator"],
            "usermode":true
        },
        {
            "name":"stegseek",
            "image":"stegano",
            "command":["stegseek"],
            "usermode":false
        },
        {
            "name":"exiftool",
            "image":"stegano",
            "command":["exiftool"],
            "usermode":false
        },
        {
            "name":"wpscan",
            "image":"wpscanteam/wpscan",
            "command":["wpscan"],
            "usermode":false
        },
        {
            "name":"wfuzz",
            "image":"webrecon",
            "command":["wfuzz"],
            "usermode":false
        },
        {
            "name":"steghide",
            "image":"stegano",
            "command":["steghide"],
            "usermode":false
        },
        {
            "name":"ffuf",
            "image":"go",
            "command":["ffuf"],
            "usermode":false
        },
        {
            "name":"gobuster",
            "image":"go",
            "command":["gobuster"],
            "usermode":false
        },
        {
            "name":"msfconsole",
            "image":"metasploit",
            "command":["/opt/metasploit-framework/bin/msfconsole"],
            "usermode":false
        },
        {
            "name":"msfvenom",
            "image":"metasploit",
            "command":["/opt/metasploit-framework/bin/msfvenom"],
            "usermode":false
        },
        {
            "name":"sqlmap",
            "image":"sqlmap",
            "command":[""],
            "usermode":false
        },
        {
            "name":"sslscan",
            "image":"webrecon",
            "command":["sslscan"],
            "usermode":false
        },
        {
            "name":"binwalk",
            "image":"stegano",
            "command":["binwalk","--run-as=root"],
            "usermode":false
        },
        {
            "name":"volatility",
            "image":"pwnrev",
            "command":["python3","/opt/volatility3/vol.py"],
            "usermode":false
        },
        {
            "name":"jwt_tool",
            "image":"jwt",
            "command":["python3","/opt/jwt_tool/jwt_tool.py"],
            "dockerarg":["--mount","type=bind,source=$HOME/.config/jwt_tool/jwtconf.ini,target=/opt/jwt_tool/jwtconf.ini"],
            "localcmd":["mkdir","-p","~/.config/jwt_tool","&&","cp","-u","./jwt/jwtconf.ini","~/.config/jwt_tool/jwtconf.ini"],
            "usermode":false
        },
        {
            "name":"rsactftool",
            "image":"crypto",
            "command":["python3","/opt/rsactftool/RsaCtfTool.py"],
            "usermode":false
        },
        {
            "name":"rshack",
            "image":"crypto",
            "command":["python3","/opt/rshack/rshack.py"],
            "usermode":false
        }
    ]
}